mixin blogCard(data)
  a.custom-card__arrow(href="")
  .expert__img(style=`background-image:url(${data.img})`) 
  .expert__content
    div
      .expert__date=data.date
      a.expert__title(href="")=data.title
    .custom__tags
      a.tag(href="") # SEO продвижение сайта
      a.tag(href="") # Редизайн сайта 